             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="450" d:DesignHeight="20"
             x:Class="DeltaEditorAvalonia.HierarchyNodeControl">

  <Border Background="Magenta"
          Height="20">
